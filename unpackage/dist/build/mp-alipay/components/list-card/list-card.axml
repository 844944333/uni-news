<view data-event-opts="{{[['tap',[['open',['$event']]]]]}}" onTap="__e"><block a:if="{{item.mode==='base'}}"><view class="listcard"><view class="listcard-img"><image src="{{item.cover[0]}}" mode></image></view><view class="listcard-content"><view class="listcard-content__titel"><text>{{item.title}}</text><like vue-id="a32577d0-1" types="{{types}}" item="{{item}}" onVueInit="__l"></like></view><view class="listcard-content__des"><text class="listcard-content__des-label">{{item.classify}}</text><text class="listcard-content__des-browse">{{item.browse_count+"浏览"}}</text></view></view></view></block><block a:if="{{item.mode==='column'}}"><view class="listcard column"><view class="listcard-content"><view class="listcard-content__titel"><text>{{item.title}}</text><like vue-id="a32577d0-2" types="{{types}}" item="{{item}}" onVueInit="__l"></like></view><view class="listcard-image"><block a:for="{{item.cover}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{index<3}}"><view class="listcard-img"><image src="{{item}}" mode></image></view></block></block></view><view class="listcard-content__des"><text class="listcard-content__des-label">{{item.classify}}</text><text class="listcard-content__des-browse">{{item.browse_count+"浏览"}}</text></view></view></view></block><block a:if="{{item.mode==='image'}}"><view class="listcard image"><view class="listcard-image"><image src="{{item.cover[0]}}" mode></image></view><view class="listcard-content"><view class="listcard-content__titel"><text>{{item.title}}</text><like vue-id="a32577d0-3" types="{{types}}" item="{{item}}" onVueInit="__l"></like></view><view class="listcard-content__des"><text class="listcard-content__des-label">{{item.classify}}</text><text class="listcard-content__des-browse">{{item.browse_count+"浏览"}}</text></view></view></view></block></view>