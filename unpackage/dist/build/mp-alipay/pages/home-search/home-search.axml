<view class="home"><navbar vue-id="8c37d7b0-1" isSearch="{{true}}" value="{{value}}" data-event-opts="{{[['^input',[['__set_model',['','value','$event',[]]],['change']]]]}}" onInput="__e" onVueInit="__l"></navbar><view class="home-list"><block a:if="{{is_history}}"><view class="label-box"><view class="label-header"><text class="label-title">搜索历史</text><text data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" class="label-clear" onTap="__e">清空</text></view><block a:if="{{historyLists.length>0}}"><view class="label-content"><view data-event-opts="{{[['tap',[['openHistory',['$0'],[[['historyLists','',index]]]]]]]}}" class="label-content__item" onTap="__e" a:for="{{historyLists}}" a:for-item="item" a:for-index="index" a:key="index">{{''+item.name+''}}</view></view></block><block a:else><view class="no-data">没有搜索历史</view></block></view></block><block a:else><scroll-view class="list-scroll" scroll-y="{{true}}"><block a:if="{{loading}}"><uni-load-more vue-id="8c37d7b0-2" status="loading" iconType="snow" onVueInit="__l"></uni-load-more></block><block a:if="{{searchList.length>0}}"><view><list-card vue-id="{{'8c37d7b0-3-'+index}}" item="{{item}}" data-event-opts="{{[['^click',[['setHistory']]]]}}" onClick="__e" a:for="{{searchList}}" a:for-item="item" a:for-index="index" a:key="index" onVueInit="__l"></list-card></view></block><block a:if="{{searchList.length===0&&!loading}}"><view class="no-data">没有搜索到相关数据</view></block></scroll-view></block></view></view>