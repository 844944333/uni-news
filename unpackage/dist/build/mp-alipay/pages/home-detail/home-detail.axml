<view class="detail"><view class="detail-title"><text>{{formData.title}}</text></view><view class="detail-header"><view class="detail-header__logo"><image src="{{formData.author.avatar}}" mode></image></view><view class="detail-header__content"><view class="detail-header__content-title">{{''+formData.author.author_name+''}}</view><view class="detail-header__content-info"><text>{{formData.create_time}}</text><text>{{formData.browse_count+"浏览"}}</text><text>{{formData.thumbs_up_count+"赞"}}</text></view></view><u-button class="detail-header__button" vue-id="78cf7796-1" type="success" ripple="{{true}}" shape="square" data-event-opts="{{[['^click',[['follow',['$0'],['formData.author.id']]]]]}}" onClick="__e" onVueInit="__l">{{formData.is_author_like?"已关注":"关注"}}</u-button></view><view class="detail-content"><view class="detail-html"><u-parse vue-id="78cf7796-2" content="{{formData.content}}" noData="{{noData}}" onVueInit="__l"></u-parse></view><view class="detail-comment"><view class="comment-title">最新评论</view><view class="comment-content" a:for="{{commentsList}}" a:for-item="item" a:for-index="__i0__" a:key="comment_id"><comments-box vue-id="{{'78cf7796-3-'+__i0__}}" comments="{{item}}" data-event-opts="{{[['^reply',[['reply']]]]}}" onReply="__e" onVueInit="__l"></comments-box></view></view></view><view class="detail-bottom"><view data-event-opts="{{[['tap',[['openComment',['$event']]]]]}}" class="detail-bottom_input" onTap="__e"><text>谈谈你的看法</text><uni-icons vue-id="78cf7796-4" type="compose" size="16" color="#f07373" onVueInit="__l"></uni-icons></view><view class="detail-bottom_icons"><view data-event-opts="{{[['tap',[['open',['$event']]]]]}}" class="detail-bottom_icons-box" onTap="__e"><uni-icons vue-id="78cf7796-5" type="chat" size="22" color="#F07373" onVueInit="__l"></uni-icons></view><view data-event-opts="{{[['tap',[['clickHeart',['$0'],['formData._id']]]]]}}" class="detail-bottom_icons-box" onTap="__e"><uni-icons vue-id="78cf7796-6" type="{{formData.is_like?'heart-filled':'heart'}}" size="22" color="#F07373" onVueInit="__l"></uni-icons></view><view data-event-opts="{{[['tap',[['thumbsup',['$0'],['formData._id']]]]]}}" class="detail-bottom_icons-box" onTap="__e"><uni-icons vue-id="78cf7796-7" type="{{formData.is_thumbs_up_article?'hand-thumbsup-filled':'hand-thumbsup'}}" size="22" color="#F07373" onVueInit="__l"></uni-icons></view></view></view><u-popup onInput="__e" vue-id="78cf7796-8" mode="bottom" mask-close-able="{{true}}" value="{{show}}" data-event-opts="{{[['^input',[['__set_model',['','show','$event',[]]]]]]}}" onVueInit="__l"><view class="popup-wrap"><view class="popup-header"><text data-event-opts="{{[['tap',[['close',['$event']]]]]}}" onTap="__e">取消</text><text data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" onTap="__e">发布</text></view><view class="popup-content"><textarea class="popup-textarea" placeholder="请输入评论内容" maxlength="200" data-event-opts="{{[['input',[['__set_model',['','value','$event',[]]]]]]}}" value="{{value}}" onInput="__e"></textarea><view class="popup-count">{{''+value.length+'/200'}}</view></view></view></u-popup><u-back-top vue-id="78cf7796-9" scroll-top="{{scrollTop}}" top="600" icon-style="{{iconStyle}}" duration="{{500}}" onVueInit="__l"></u-back-top></view>